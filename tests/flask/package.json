{
  "name": "devpail-test-flask",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devpail": {
    "moduleCDN": "http://host.docker.internal:8000",
    "compilers": [
      {
        "gulp": "compiler/javascript/uglify"
      },
      {
        "gulp": "compiler/css/clean-css"
      }
    ],
    "ports": [
      "4000:3000"
    ],
    "servers": [
      {
        "gulp": "server/flask",
        "source": "flaskapp",
        "entrypoint": "application.py",
        "port": 1,
        "environ": {}
      }
    ],
    "tasks": [
      {
        "names": ["deploy", "deploy:production", "deploy:config"],
        "gulp": "task/deploy/aws-elasticbeanstalk",
        "create": {
          "keyname": "aws-eb",
          "platform": "python-3.8",
          "region": "us-east-1"
        },
        "config": {
          "branch-defaults": {
            "staging": {
              "environment": "devpail-test-flask-staging"
            },
            "production": {
              "environment": "devpail-test-flask-production"
            }
          },
          "global": {
            "application_name": "devpail-test-flask",
            "default_ec2_keyname": "aws-eb",
            "default_platform": "Python 3.8 running on 64bit Amazon Linux 2",
            "default_region": "us-east-1",
            "workspace_type": "Application"
          }
        }
      }
    ],
    "imageTag": "default"
  },
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "del": "^6.0.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "merge-options": "^3.0.4",
    "require-from-url": "^3.1.3",
    "yaml": "^1.10.2"
  }
}
